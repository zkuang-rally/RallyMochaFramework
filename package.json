{
    "name": "rallyautomation",
    "version": "1.0.0",
    "description": "Rally Automation",
    "main": "index.js",
    "scripts": {
        "test:testrail": "TEST_RAIL_UPDATE=true npx wdio wdio.conf.js",
        "test:testrailSauce": "SAUCE_LABS=true BROWSER=ie TEST_RAIL_UPDATE=true npx wdio wdio.conf.js",
        "test": "npx wdio run ./wdio.conf.js && ./node_modules/.bin/allure generate allure-results --clean && allure open",
        "Regression": "npx wdio run ./wdio.conf.js --suite Regression && ./node_modules/.bin/allure generate allure-results --clean && allure open",
        "allureReport": "./node_modules/.bin/allure generate allure-results --clean && allure open"
    },
    "author": "Laxmiraj Uppula",
    "license": "ISC",
    "dependencies": {
        "@wdio/cli": "^7.7.4",
        "@wdio/sync": "^7.9.0",
        "allure-commandline": "^2.13.8",
        "base-64": "^1.0.0",
        "chai-webdriverio": "^1.0.0",
        "del": "^6.0.0",
        "moment": "^2.29.1",
        "path": "^0.12.7",
        "sync-request": "^6.1.0",
        "xlsx": "^0.17.0"
    },
    "devDependencies": {
        "@wdio/allure-reporter": "^7.9.0",
        "@wdio/local-runner": "^7.9.0",
        "@wdio/mocha-framework": "^7.9.0",
        "@wdio/spec-reporter": "^7.9.0",
        "chai": "^4.3.4",
        "chromedriver": "^95.0.0",
        "mocha-testrail-reporter": "^1.0.12",
        "wdio-chromedriver-service": "^7.2.0",
        "wdio-testrail-reporter": "^1.1.5",
        "@wdio/selenium-standalone-service": "^7.12.5"
    }
}
